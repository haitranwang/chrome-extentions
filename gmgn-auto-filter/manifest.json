{
  "manifest_version": 3,
  "name": "GMGN Auto Filter",
  "version": "1.0.0",
  "description": "GMGN extension: Auto-open token tabs from top-ranked tokens with cooldown management for Solana and BSC chains",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://gmgn.ai/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://gmgn.ai/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}

